{
	"extends": "airbnb",
	"parser": "babel-eslint",
	"parserOptions": {
		"experimentalDecorators": true
	},
	"rules": {
        "jsx-a11y/anchor-is-valid": "off",
		"indent": ["error", "tab"],
		"no-tabs": "off",
		"react/jsx-indent": ["error", "tab"],
        "react/jsx-indent-props": ["error", "tab"],
        "comma-dangle": ["error", "never"],
        "import/prefer-default-export": "off"
	},
	"env": {
		"browser": true
	}
}
